.mermaid{background-color:#2d2d2d;border-radius:8px;padding:20px;margin:20px 0;box-shadow:0 4px 6px rgba(0,0,0,.1)}.mermaid svg{display:block;margin:0 auto}.mermaid .node rect,.mermaid .node circle,.mermaid .node ellipse,.mermaid .node polygon,.mermaid .node path{stroke-width:2px;stroke:#7f7f7f;fill:#3d3d3d}.mermaid .label{color:#fff}.mermaid .edgeLabel{background-color:#2d2d2d}.mermaid .cluster rect{fill:#3d3d3d!important;stroke:#888!important;stroke-width:1px!important}.mermaid .cluster text{fill:#f8f8f2}